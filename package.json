{
  "name": "circle-open",
  "version": "0.1.1",
  "homepage": "https://gosha.github.io/react-reveal-circle/",
  "repository": "https://github.com/Gosha/react-reveal-circle",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "d3-ease": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.1.0",
    "react-spring": "^9.4.5"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "true",
    "deploy": "yarn build && cd build && git init && git remote add origin git@github.com:Gosha/react-reveal-circle.git && git add . && git commit -m deploy && git push -f --set-upstream origin master",
    "eject": "react-scripts eject",
    "postinstall": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/d3-ease": "^3.0.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.3",
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react-scripts": "5.0.1",
    "typescript": "~4.7.4"
  }
}
