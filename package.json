{
  "name": "circle-open",
  "version": "0.1.1",
  "homepage": "https://gosha.github.io/react-reveal-circle/",
  "repository": "https://github.com/Gosha/react-reveal-circle",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.1.1",
    "d3-ease": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-spring": "^8.0.27"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "true",
    "deploy": "yarn build && cd build && git init && git remote add origin git@github.com:Gosha/react-reveal-circle.git && git add . && git commit -m deploy && git push -f --set-upstream origin master",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/d3-ease": "^2.0.0",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "husky": "^4.3.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-scripts": "3.4.4",
    "typescript": "~4.1.2"
  }
}
